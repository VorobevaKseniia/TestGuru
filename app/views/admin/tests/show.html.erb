<p><strong><%= t('.title', title: @test.title) %></strong></p>
<p><strong><%= t('.level', level: @test.level) %></strong></p>
<p>
  <%= link_to t('admin.links.new_question'), new_admin_test_question_path(@test) %>
</p>

<% if @test.questions.present? %>
  <table>
    <tr>
      <th><%= t('.questions') %></th>
      <th colspan = '3'><%= t('.actions') %></th>
    </tr>
    <tr>
      <th>
        <%= render @test.questions %>
      </th>

    </tr>
  </table>
<% else %>
  <p>Error</p>
<% end %>
<%= link_to t('admin.links.edit'), edit_admin_test_path(@test) %> |
<%= link_to t('admin.links.back'), admin_tests_path %>