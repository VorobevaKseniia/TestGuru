<tbody>
  <tr>
    <td><%= test.title %></td>
    <td><%= test_level(test) %></td>
    <td><%= test.questions.count %></td>
    <td>
      <%= link_to t('admin.actions.show'), admin_test_path(test), class: "fw-bolder link-secondary text-decoration-none" %> |
      <%= link_to t('admin.actions.edit'), edit_admin_test_path(test), class: "fw-bolder link-secondary text-decoration-none" %> |
      <%= link_to t('admin.actions.delete'),
                    admin_test_path(test),
                    method: :delete,
                    data: {confirm: t('.confirm_delete')},
                    class: "fw-bolder link-secondary text-decoration-none"%>
    </td>
  </tr>
</tbody>

